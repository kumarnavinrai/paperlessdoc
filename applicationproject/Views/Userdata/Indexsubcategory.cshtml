@model IEnumerable<applicationproject.Models.Subcategories>

@{
    ViewBag.Title = "Select Subcategory";
}

<h2>Select Subcategory</h2>
<p>Language : @ViewBag.languagename</p>
<p>Country : @ViewBag.countryname</p>
<p>State : @ViewBag.statename</p>
<p>District : @ViewBag.districtname</p>
<p>Department : @ViewBag.departmentname</p>
<p>Category : @ViewBag.categoryname</p>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.config)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.config)
        </td>
       
        <td>
            @Html.ActionLink("Select Document", "Indexdocument", new { id = item.subcategoryId, countryname = ViewBag.countryName, statename = ViewBag.stateName, districtname = ViewBag.districtname, departmentname = ViewBag.departmentname, categoryname = ViewBag.categoryname, languagename = ViewBag.languagename, subcategoryname = item.name, languageid = ViewBag.languageId, countryid = ViewBag.countryId, stateid = ViewBag.stateid, districtid = ViewBag.districtid, departmentid = ViewBag.departmentid, categoryid = ViewBag.categoryid }) 
        </td>
    </tr>
}

</table>
